    "import random\n",
    "\n",
    "filename = \"FINAL.csv\" \n",
    "n = sum(1 for line in open(filename))-1  # Calculate number of rows in file\n",
    "s = n//50  # sample size of 2%\n",
    "skip = sorted(random.sample(range(1, n+1), n-s))  # n+1 to compensate for header "
   ]
